/*******************************
        User Overrides
*******************************/

.ui.dropdown, .ui.selection.dropdown {
  border-right: none;
  border-left: none;
  border-top: none;
}

.ui.selection.dropdown {
  & > .text {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    width: 100%;
  }
}